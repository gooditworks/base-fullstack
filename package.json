{
  "name": "gooditworks_base_fullstack",
  "version": "0.2.3",
  "description": "Gooditworks base fullstack project",
  "repository": "https://github.com/gooditworks/base-fullstack",
  "license": "MIT",
  "author": "Max Eliseev <mxseev@gmail.com>",
  "scripts": {
    "build": "next build",
    "codegen": "concurrently -c green \"npm:codegen:*\"",
    "codegen:prisma": "npm run prisma:generate",
    "commit": "git add . && cz",
    "dev": "next dev",
    "postinstall": "sh ./scripts/add_base_remote.sh && npm run codegen",
    "lint": "concurrently -c green \"npm:lint:*\"",
    "lint:base": "sh ./scripts/check_base_update.sh",
    "lint:next": "next lint",
    "lint:typecheck": "tsc --noEmit --project tsconfig.json",
    "playwright:inspect": "PWDEBUG=1 playwright test",
    "playwright:install": "playwright install",
    "playwright:test": "playwright test",
    "playwright:update-snapshots": "playwright test --update-snapshots",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "release": "standard-version",
    "start": "next build && next start",
    "style:packagesort": "sort-package-json",
    "test": "concurrently -c green \"npm:test:*\"",
    "test:e2e": "npm run playwright:test",
    "test:unit": "jest"
  },
  "dependencies": {
    "@gooditworks/monitoring": "1.5.4",
    "@prisma/client": "4.4.0",
    "@trpc/client": "9.27.4",
    "@trpc/next": "9.27.4",
    "@trpc/react": "9.27.4",
    "@trpc/server": "9.27.4",
    "classnames": "2.3.2",
    "next": "12.3.1",
    "next-axiom": "0.14.0",
    "prisma": "4.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.2",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@arkweid/lefthook": "0.7.7",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@playwright/test": "1.26.1",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.1.1",
    "@types/node": "17.0.5",
    "@types/react": "18.0.21",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "autoprefixer": "10.4.12",
    "commitizen": "4.2.5",
    "commitlint-plugin-function-rules": "1.7.1",
    "concurrently": "7.4.0",
    "cssnano": "5.1.13",
    "cz-conventional-changelog": "3.3.0",
    "enhanced-resolve": "5.10.0",
    "eslint": "8.24.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "12.3.1",
    "eslint-plugin-jest": "27.1.1",
    "jest": "29.1.2",
    "jest-environment-jsdom": "29.1.2",
    "jest-junit": "14.0.1",
    "postcss": "8.4.17",
    "postcss-flexbugs-fixes": "5.0.2",
    "prettier": "2.7.1",
    "sort-package-json": "2.0.0",
    "standard-version": "9.5.0",
    "tailwindcss": "3.1.8",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
